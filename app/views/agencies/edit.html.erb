<h1>Editing agency</h1>

<% form_for(@agency) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :a_name %><br />
    <%= f.text_field :a_name %>
  </p>
  <p>
    <%= f.label :a_address %><br />
    <%= f.text_field :a_address %>
  </p>
  <p>
    <%= f.label :a_contact_no %><br />
    <%= f.text_field :a_contact_no %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @agency %> |
<%= link_to 'Back', agencies_path %>